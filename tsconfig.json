{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./build/",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "typeRoots": [
      "src/types/index.d.ts"
    ]
  },
  "exclude": [
    "build",
  ]
}
