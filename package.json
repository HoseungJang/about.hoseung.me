{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.2.1",
    "react-icons": "^3.11.0",
    "react-intersection-observer": "^8.29.1",
    "styled-components": "^5.2.0",
    "use-media": "^1.4.0"
  },
  "scripts": {
    "deploy": "npm run build && npm run remove && npm run upload",
    "remove": "aws s3 rm s3://about-hoseung --recursive --profile hsjang",
    "upload": "aws s3 sync ./build s3://about-hoseung --acl public-read --profile hsjang",
    "build": "webpack --env mode=production",
    "dev": "webpack serve --env mode=development",
    "start": "run-p dev format:watch",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:watch": "onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/classnames": "^2.2.11",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.11",
    "typescript": "^3.7.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
